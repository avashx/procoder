{
  "rewrites": [
    {
      "source": "/api/websocket",
      "destination": "/api/server"
    },
    {
      "source": "/wifi",
      "destination": "/wifi.html"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "functions": {
    "api/server.js": {
      "memory": 128,
      "maxDuration": 10
    }
  }
}